<config xmlns="http://tail-f.com/ns/config/1.0">
  <devices xmlns="http://tail-f.com/ns/ncs">
    <device>
      <name>nso-1</name>
      <authgroup>default</authgroup>
      <lsa-remote-node>nso-1</lsa-remote-node>
      <device-type>
        <netconf xmlns:ned="http://tail-f.com/ns/ncs-ned">
          <ned-id
              xmlns:id="http://tail-f.com/ns/ned-id/%%MNAME%%"
              >id:%%MNAME%%</ned-id>
        </netconf>
      </device-type>
      <state>
        <admin-state>unlocked</admin-state>
      </state>
    </device>
    <device>
      <name>nso-2</name>
      <authgroup>default</authgroup>
      <lsa-remote-node>nso-2</lsa-remote-node>
      <device-type>
        <netconf xmlns:ned="http://tail-f.com/ns/ncs-ned">
          <ned-id
              xmlns:id="http://tail-f.com/ns/ned-id/%%MNAME%%"
              >id:%%MNAME%%</ned-id>
        </netconf>
      </device-type>
      <state>
        <admin-state>unlocked</admin-state>
      </state>
    </device>
    <device>
      <name>nso-3</name>
      <authgroup>default</authgroup>
      <lsa-remote-node>nso-3</lsa-remote-node>
      <device-type>
        <netconf xmlns:ned="http://tail-f.com/ns/ncs-ned">
          <ned-id
              xmlns:id="http://tail-f.com/ns/ned-id/%%MNAME%%"
              >id:%%MNAME%%</ned-id>
        </netconf>
      </device-type>
      <state>
        <admin-state>unlocked</admin-state>
      </state>
    </device>
  </devices>
</config>
