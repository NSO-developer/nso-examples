  augment "/ncs:devices/ncs:device/ncs:config/r:sys" {
    container router-config {
      tailf:ncs-device-type "generic-ned";
      tailf:display-when "not(../../../ncs:module) or ../../../ncs:module[ncs:name='router-config']";
      leaf want-reverse {
        type boolean;
