        // NCS drop default statement
      }
    }
  }
  augment "/ncs:devices/ncs:template/ncs:ned-id/ncs:config/r:sys" {
    container router-config {
      leaf want-reverse {
        type string;
        // NCS drop default statement
      }
      leaf trans-id-mode {
        type string;
        // NCS drop default statement
      }
    }
  }
  augment "/ncs:devices/ncs:device/ncs:live-status/r:sys" {
    container router-config {
      tailf:ncs-device-type "generic-ned";
