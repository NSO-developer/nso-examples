module netsim-ned-cisco-ios-xr-stats {
  namespace "http://tail-f.com/ned/cisco-ios-xr-stats";
  prefix cisco-ios-xr-stats;

  import tailf-common {
    prefix tailf;
  }

  list inventory {
    config false;
    key "name";
    leaf name {
      type string;
    }
    leaf descr {
      type string;
    }
    leaf pid {
      type string;
    }
    leaf vid {
      type string;
    }
    leaf sn {
      type string;
    }
    tailf:alt-name "inventory-stats";
  }
  container cdp {
    config false;
    list neighbors {
      key "local-interface";
      leaf local-interface {
        type string;
      }
      leaf device-id {
        type string;
      }
      leaf hold-time {
        type string;
      }
      leaf capability {
        type string;
      }
      leaf platform {
        type string;
      }
      leaf port-id {
        type string;
      }
    }
    tailf:alt-name "cdp-stats";
  }
  container lldp {
    config false;
    list neighbors {
      key "local-interface";
      leaf local-interface {
        type string;
      }
      leaf parent-interface {
        type string;
      }
      leaf device-id {
        type string;
      }
      leaf time-remaining {
        type string;
      }
      leaf hold-time {
        type string;
      }
      leaf capability {
        type string;
      }
      leaf chassis-id {
        type string;
      }
      leaf port-id {
        type string;
      }
    }
    tailf:alt-name "lldp-stats";
  }
  container controllers {
    config false;
    list Optics {
      key "id";
      leaf id {
        type string;
      }
      container instance {
        tailf:cli-drop-node-name;
        presence "true";
        leaf controller-state {
          type string;
        }
        container transceiver-vendor-details {
          leaf form-factor {
            type string;
          }
          leaf optics-type {
            type string;
          }
          leaf name {
            type string;
          }
          leaf oui-number {
            type string;
          }
          leaf part-number {
            type string;
          }
          leaf rev-number {
            type string;
          }
          leaf serial-number {
            type string;
          }
          leaf pid {
            type string;
          }
          leaf vid {
            type string;
          }
          leaf date-code {
            type string;
          }
        }
      }
    }
    tailf:alt-name "controllers-stats";
  }
  container exec {
    tailf:alt-name "exec-stats";
  }
}
